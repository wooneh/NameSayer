	<Label style="-fx-text-fill: white; -fx-font-size: 40px;" text="Welcome to" />
	<Label style="-fx-text-fill: green; -fx-font-size: 60px;" text="NameSayer" />
	<Button fx:id="startButton" style="-fx-font-size: 40px;" text="Start" />
   <Button mnemonicParsing="false" onAction="#helpButtonAction" text="Help">
      <font>
         <Font size="25.0" />
      </font>
   </Button>
</VBox>
